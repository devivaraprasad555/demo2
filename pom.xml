<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
             http://maven.apache.org/xsd/settings-1.0.0.xsd">
       
       <servers>
           <server>
               <id>nexus-releases</id> <!-- Match this with the <id> in your pom.xml -->
               <username>admin</username> <!-- Your Nexus username -->
               <password>admin</password> <!-- Your Nexus password -->
           </server>
       </servers>

       <profiles>
           <profile>
               <id>nexus</id>
               <repositories>
                   <repository>
                       <id>nexus-releases</id>
                       <url>http://52.91.81.45:8081/repository/java-packages/</url>
                       <releases>
                           <enabled>true</enabled>
                       </releases>
                       <snapshots>
                           <enabled>false</enabled>
                       </snapshots>
                   </repository>
               </repositories>
           </profile>
       </profiles>
       <activeProfiles>
           <activeProfile>nexus</activeProfile>
       </activeProfiles>
   </settings>
